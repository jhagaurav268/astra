                                                                                            </div>
                                                                                            <apex:commandButton value="Submit" styleClass="submitButton" 
                                                                                                                status="submitStatus" disabled="false"/>
                                                                                        </apex:outputPanel>
                                                                                    </apex:facet>
                                                                                </apex:actionStatus>    
                                                                                <apex:commandButton value="Cancel" action="{!closePopup}" styleclass="submitButton btn btn-warning"  onclick="hideDiv('divPopup');return false;" />
                                                                            </apex:outputPanel>
                                                                        </footer>
                                                                    </div>
                                                                </div>
                                                            </section>
