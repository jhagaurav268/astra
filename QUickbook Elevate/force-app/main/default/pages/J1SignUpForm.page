            function isNumber(evt) {
                
                var numb = evt.target.value;
                
                evt = (evt) ? evt : window.event;
                var charCode = (evt.which) ? evt.which : evt.keyCode;
                
                if (charCode == 8 || charCode == 46 || charCode == 37 || charCode == 38 || charCode == 39 || charCode == 40 || charCode == 9 || (charCode > 45 && charCode < 58) || (charCode > 95 && charCode < 106)) {
                    return true;
                } else {
                    return false;
                }
                
            } 
            
            function validateEmail(mailField) 
            {
                
                if(mailField!=null && mailField!=undefined){
                    var mailformat = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
                    
                    if (mailField.value.match(mailformat))
                    {
                        return (true)
                    }else{
                        mailField.focus();
                        $("#divError h2").each(function() {
                            $(this).text('Please enter a valid email Id in format abc@mail.com.');
                        });
                        $("#divError").removeClass('slds-hide');
                        setTimeout(function() {
                            $("#divError").addClass('slds-hide');
                        }, 10000); 
                        return false;
                    }
                    
                }
                
            }
            
            function replaceChars(evt) {
                //console.log(evt);
                if (!!/\D/.test(evt.target.value))  {
                    var origin= evt.target.value;
                    var newVal= origin.replace(/[^0-9 ]/g, "");
                    // alert(newVal);
                    evt.target.value=newVal;
                    return false;
                }            
            }  
            
            function replaceCharsInDOB(evt) {
                
                var origin= evt.target.value;
                var listNos=[];
                if(origin.length>0){
                    listNos = origin.split('/');
                }
                var newVal= origin.replace(/[^0-9 ]/g, "");
                if(newVal.length >= 2 && newVal.length < 4){
                    
                    if(!(isNaN(newVal))){
                        if(origin.length==3){
                            document.getElementById("dob").value = newVal.substring(0,2)+"/" +newVal.substring(2,newVal.length); 
                            
                        }
                        else if(origin.length>3){
                            document.getElementById("dob").value = newVal.substring(0,2)+"/" +newVal.substring(2,newVal.length); 
                        }
                            else{
                                document.getElementById("dob").value = newVal.substring(0,2);
                            }
                        
                    }
                    
                }else if(newVal.length == 4){
                    if(!(isNaN(newVal))){
                        if(origin.length==6){
                            document.getElementById("dob").value = newVal.substring(0,2)+"/" +newVal.substring(2,newVal.length)+"/"; 
                        }
                        if(origin.length==5){
                            document.getElementById("dob").value = newVal.substring(0,2)+"/" +newVal.substring(2,newVal.length); 
                        }
                        
                    }
                    
                }else  if(newVal.length > 4 ){
                    if(!(isNaN(newVal))){
                        if(listNos.length!=3){
                            document.getElementById("dob").value = newVal.substring(0,2)+"/" +newVal.substring(2,4)+"/" +newVal.substring(4,newVal.length); 
                        }
                        
                    }
                    
                } else{
                    evt.target.value=newVal;
                }    
            }   
            
            
            
