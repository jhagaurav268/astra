            function checkDatePins(datVal){
                var originalVal= JSON.parse(JSON.stringify(datVal));
                var temp=datVal;
                if(temp.includes("/")){
                    temp= temp.replace('/','');
                }
                if(typeof temp!='number'){
                    return false;
                }
                if(datVal.length == 2 && parseInt(datVal) > 12){
                    document.getElementById("dob").value = datVal ; 
                    return false ;
                }
                
                if(datVal.length == 5 && parseInt(datVal.substring(3,5)) > 31){
