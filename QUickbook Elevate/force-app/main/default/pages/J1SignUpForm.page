                                                                                                            status="submitStatus"  ReRender="errorPanel,finalPanel,mainPanel,containerPanel" styleclass="submitButton btn btn-warning" >
                                                                                        </apex:commandButton>   
                                                                                        
                                                                                    </apex:facet>
                                                                                    
                                                                                    <apex:facet name="start">
                                                                                        <apex:outputPanel >
                                                                                            <div style="position: fixed; top: 0; left: 0; right: 0; bottom: 0; opacity: 0.75; z-index: 1000; background-color: black;">
                                                                                                &nbsp;
                                                                                            </div>
                                                                                            <div style="position: fixed; left: 0; top: 0; bottom: 0; right: 0; z-index: 1001;  margin-top:40%;">
